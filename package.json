{
  "name": "fork-spira-vscode",
  "displayName": "fork-spira-vscode",
  "description": "Testing for a new version of Spira VSCode Extension",
  "icon": "media/test_icon.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fork-spira-vscode.addTask",
        "title": "Add Task"
      },
      {
        "command": "fork-spira-vscode.verifyCred",
        "title": "Verify Credentials"
      }
    ],
    "viewsContainers": {
      "activitybar": [
          {
              "id": "testing-extension",
              "title": "Testing",
              "icon": "media/test_icon.png"
          }
      ]
    },
    "views": {
      "testing-extension": [
          {
              "id": "testing-extension",
              "name": "My Assigned"
          }
      ]
  }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/superagent": "^4.1.18",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "superagent": "^8.0.9"
  }
}
